{
  "name": "branch-locator",
  "version": "0.5.0",
  "scripts": {
    "preversion": "npm install && npm run build && npm run patch-lib && git add .",
    "start": "ng serve --open",
    "build": "npm run test-lib-prod && npm run build-lib && node ./tools/readme-coverage.js",
    "build-dev": "npm run test-lib-dev && npm run build-lib && node ./tools/readme-coverage.js",
    "build-local": "ng build --base-href ./",
    "build-pro": "ng build --prod --aot --base-href ./ && node tools/assets-fix.js LOCAL",
    "build-lib": "ng lint sn-branch-locator && ng build sn-branch-locator && node tools/assets-fix.js",
    "dev-lib": "ng lint sn-branch-locator && ng build sn-branch-locator --watch",
    "test": "ng test",
    "test-lib": "ng test sn-branch-locator",
    "test-lib-prod": "npm run lint-lib && BROWSER=NONE ng test sn-branch-locator --watch=false",
    "test-lib-dev": "npm run lint-lib && SET BROWSER=NONE && ng test sn-branch-locator --watch=true",
    "lint": "ng lint",
    "lint-lib": "ng lint sn-branch-locator",
    "e2e": "ng e2e",
    "patch-lib": "cd libs/sn-branch-locator && npm version patch",
    "prebuild": "echo ko > .buildstatus",
    "postbuild-lib": "echo ok > .buildstatus"
  },
  "private": true,
  "modules-publish": [
    "sn-branch-locator"
  ],
  "dependencies": {
    "@agm/core": "^1.0.0-beta.7",
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@globile/mobile-services": "0.0.5",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.3.3",
    "sn-common-lib": "~0.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/googlemaps": "^3.37.7",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "husky": "^3.0.7",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "prismjs": "^1.17.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-lib"
    }
  }
}
