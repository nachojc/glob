{
  "name": "branch-locator",
  "version": "1.0.26",
  "scripts": {
    "preversion": "npm run patch-lib && git add .",
    "start": "ng serve --open",
    "build": "npm run build-lib",
    "build-dev": "npm run test-lib-dev && npm run build-lib",
    "build-local": "ng build --base-href /",
    "build-aot": "ng build --aot --base-href /",
    "build-lib": "ng lint sn-branch-locator && ng build sn-branch-locator && node tools/assets-fix.js",
    "dev-lib": "ng lint sn-branch-locator && ng build sn-branch-locator --watch",
    "test": "ng test",
    "test-lib": "ng test sn-branch-locator",
    "test-blgroup": "ng test BL-group",
    "test-lib-prod": "npm run lint-lib",
    "test-lib-dev": "npm run lint-lib && SET BROWSER=NONE && ng test sn-branch-locator --watch=true",
    "lint": "ng lint",
    "lint-lib": "ng lint sn-branch-locator",
    "e2e": "ng e2e",
    "patch-lib": "cd libs/sn-branch-locator && npm version patch",
    "prebuild": "echo ko > .buildstatus",
    "postbuild-lib": "echo ok > .buildstatus",
    "globile-update": "npm i @globile/mobile-services@latest sn-common-lib@latest --save"
  },
  "private": true,
  "modules-publish": [
    "sn-branch-locator"
  ],
  "dependencies": {
    "@agm/core": "^1.0.0",
    "@agm/js-marker-clusterer": "1.0.0",
    "@angular/animations": "^8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@globile/mobile-services": "^0.1.9",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "js-marker-clusterer": "^1.0.0",
    "rxjs": "~6.5.4",
    "sn-common-lib": "^0.7.20",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.2",
    "@angular-devkit/build-ng-packagr": "~0.803.23",
    "@angular/cdk": "~8.2.3",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/googlemaps": "^3.39.3",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "husky": "^4.2.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.7.1",
    "prismjs": "^1.19.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "~0.37.1",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-lib"
    }
  }
}