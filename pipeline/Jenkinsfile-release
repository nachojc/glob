def pipelineParams

if (params.OVERRIDE_JOB_PARAMETERS) {
    println params
    pipelineParams = params
} else {
    pipelineParams = [:]
}

def libraryBranch = pipelineParams.LIBRARY_BRANCH

println "Library branch ${libraryBranch}"

library "pipeline-globile@${libraryBranch}"

releaseAngular pipelineParams
